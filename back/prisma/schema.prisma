generator client {
  provider = "prisma-client-js"
  output   = "../node_modules/.prisma/client"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL_USERS")
}

model Users {
  id       String  @id @default(uuid())
  avatar   String?
  username String  @unique
  email    String  @unique
  password String
}
